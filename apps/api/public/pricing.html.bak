    // Calculator logic
    function calculateQuote() {
      const staff = parseInt(document.getElementById('calc-staff').value) || 1;
      const hours = parseInt(document.getElementById('calc-hours').value) || 4;
      const role = document.getElementById('calc-role').value;
      
      // Base rates by role
      const rates = {
        standard: 20.50,
        barista: 21.00,
        chef: 24.00,
        porter: 18.00,
        runner: 18.00
      };
      
      let baseRate = rates[role] || 20.50;
      
      // Volume discount
      const totalHours = staff * hours;
      if (totalHours >= 100) {
        baseRate -= 1.50; // Enterprise
      } else if (totalHours >= 50) {
        baseRate -= 1.00; // Volume
      }
      
      // Surcharges
      let surchargeMultiplier = 1;
      let surcharges = [];
      
      if (document.getElementById('surcharge-overnight').checked) {
        surchargeMultiplier += 0.20;
        surcharges.push('overnight +20%');
      }
      
      const total = staff * hours * baseRate * surchargeMultiplier;
      
      // Update display
      document.getElementById('calc-total').textContent = 'Â£' + total.toFixed(2);
      
      let breakdown = `${staff} staff Ã— ${hours} hours Ã— Â£${baseRate.toFixed(2)}/hr`;
      if (surcharges.length > 0) {
        breakdown += ` (${surcharges.join(', ')})`;
      }
      document.getElementById('calc-breakdown').textContent = breakdown;
      
      // Track calculator usage (if GA available)
      if (typeof gtag === 'function') {
        gtag('event', 'calculator_use', {
          'event_category': 'engagement',
          'event_label': role,
          'value': Math.round(total)
        });
      }
    }
    
    // FAQ accordion
    document.querySelectorAll('.faq-item h3').forEach(item => {
      item.addEventListener('click', () => {
        item.parentElement.classList.toggle('active');
      });
    });
    
    // Mobile menu toggle (fallback if vergo-nav.js not loaded)
    function toggleMenu() {
      const menu = document.getElementById('nav-menu');
      const toggle = document.querySelector('.menu-toggle');
      const isExpanded = menu.classList.toggle('active');
      toggle.setAttribute('aria-expanded', isExpanded);
    }
    
    function toggleDropdown(event, dropdownId) {
      event.preventDefault();
      event.stopPropagation();
      const dropdown = document.getElementById(dropdownId);
      dropdown.classList.toggle('active');
      const link = dropdown.querySelector('a[aria-haspopup]');
      if (link) {
        link.setAttribute('aria-expanded', dropdown.classList.contains('active'));
      }
    }
    
    // Initialize calculator
    calculateQuote();
  </script>
</body>
</html>
